        "@types/supertest": "2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "chai": "4.3.6",
        "chai-as-promised": "^7.1.1",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "mocha": "10.4.0",
        "nodemon": "3.1.0",
        "nyc": "15.1.0",
        "prettier": "3.2.5",
        "rimraf": "^5.0.5",
