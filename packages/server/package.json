        "@tsed/common": "^6.133.0",
        "@tsed/core": "^6.133.0",
        "@tsed/di": "^6.133.0",
        "@tsed/exceptions": "^6.133.0",
        "@tsed/json-mapper": "^6.133.0",
        "@tsed/jwks": "^6.133.0",
        "@tsed/mongoose": "^6.133.0",
        "@tsed/platform-express": "^6.133.0",
        "@tsed/schema": "^6.133.0",
        "@tsed/swagger": "^6.133.0",
        "@tsed/testing-mongoose": "^6.133.0",
        "@types/express-session": "^1.17.5",
        "@types/swagger-schema-official": "2.0.22",
        "body-parser": "1.19.0",
        "compression": "1.7.4",
        "concurrently": "5.3.0",
        "cookie-parser": "1.4.6",
        "cors": "^2.8.5",
        "cross-env": "7.0.2",
        "express": "4.18.1",
        "method-override": "^3.0.0",
        "mongoose": "^6.4.7",
        "node-uuid": "^1.4.8"
    },
    "devDependencies": {
        "@types/chai": "4.3.3",
        "@types/chai-as-promised": "7.1.5",
        "@types/cors": "^2.8.12",
        "@types/express": "4.17.14",
        "@types/http-proxy": "^1.17.9",
